{
  "base": "node_modules/semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../../static/styles/",
      "uncompressed": "../../static/styles/components/",
      "compressed": "../../static/styles/components/",
      "themes": "../../static/styles/themes/"
    },
    "clean": "../../static/"
  },
  "permission": false,
  "rtl": "No",
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "image",
    "list",
    "menu",
    "item",
    "visibility",
    "segment"
  ],
  "version": "2.1.6"
}